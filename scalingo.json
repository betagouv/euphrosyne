{
  "name": "euphrosyne",
  "stack": "scalingo-18",
  "env": {
    "DISABLE_COLLECTSTATIC": {
      "value": ""
    },
    "DJANGO_DEBUG": {
      "value": ""
    },
    "DJANGO_SECRET_KEY": {
      "generator": "secret"
    },
    "SITE_URL": {
      "generator": "url",
      "template": "%URL%"
    },
    "ALLOWED_HOSTS": {
      "value": ".scalingo.io"
    },
    "S3_BUCKET_NAME": {
      "generator": "template",
      "template": "%APP%"
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "addons": [
    {
      "plan": "postgresql-sandbox"
    }
  ]
}
