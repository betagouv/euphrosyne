{
  "name": "euphrosyne",
  "version": "1.0.0",
  "description": "Ouvrir les données de NewAglaé, l’Accélérateur Grand Louvre d'Analyses Elémentaires",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
    "build": "NODE_ENV=${NODE_ENV:-production} webpack --mode=production",
    "build:dev": "webpack --mode=development --watch"
  },
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betagouv/euphrosyne.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/betagouv/euphrosyne/issues"
  },
  "homepage": "https://github.com/betagouv/euphrosyne#readme",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "eslint": "^8.20.0",
    "eslint-plugin-jest": "^26.6.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.2",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "@gouvfr/dsfr": "^1.6.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "dotenv": "^16.0.1",
    "glob": "^8.0.3",
    "glob-all": "^3.3.0",
    "html-loader-jest": "^0.2.1",
    "mini-css-extract-plugin": "^2.6.1",
    "promise-polyfill": "8.2.3",
    "purgecss-webpack-plugin": "^4.1.3",
    "remixicon": "^2.5.0",
    "source-map-loader": "^4.0.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "whatwg-fetch": "^3.6.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended"
    ],
    "ignorePatterns": [
      "_static/**",
      "**/assets/dist/**"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": "latest"
    },
    "plugins": [
      "jest"
    ],
    "globals": {
      "django": false,
      "dsfr": false
    },
    "env": {
      "browser": true,
      "es6": true,
      "jest/globals": true,
      "node": true
    }
  }
}
